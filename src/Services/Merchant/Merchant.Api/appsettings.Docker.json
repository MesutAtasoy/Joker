{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Mongo": {
    "ConnectionString": "mongodb://nosqldata",
    "DefaultDatabaseName": "merchant-db"
  },
  "RabbitMQSettings": {
    "Url": "rabbitmq:5672",
    "Username": "guest",
    "Password": "guest",
    "Port": 5672,
    "Host": "rabbitmq"
  },
  "Serilog": {
    "ApplicationName": "Merchant API",
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Enrich": [
      "FromLogContext",
      "WithMachineName"
    ],
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "Seq",
        "Args": {
          "ServerUrl": "http://seq"
        }
      }
    ]
  },
  "ServiceDiscovery": {
    "ServiceId": "Merchant.Api-Docker",
    "ServiceName": "Merchant.Api",
    "ConsulUrl": "http://consul:8500",
    "Address": "http://merchant.api",
    "Port": 80,
    "Endpoints": [
      {
        "Url": "http://merchant.api/api/HealthCheck/api-status"
      }
    ]
  }
}
