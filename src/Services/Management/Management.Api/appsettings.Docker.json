{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "ConnectionString": "Server=postgres;Port=5432;Database=Joker.Management;User Id=postgres;Password=postgres;",
  "Serilog": {
    "ApplicationName": "Management API",
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Enrich": [
      "FromLogContext",
      "WithMachineName"
    ],
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "Seq",
        "Args": {
          "ServerUrl": "http://seq"
        }
      }
    ]
  },
  "ServiceDiscovery": {
    "ServiceId": "Management.Api-Docker",
    "ServiceName": "Management.Api",
    "ConsulUrl": "http://consul:8500",
    "Address": "management.api",
    "Port": 80,
    "Endpoints": [
      {
        "Url": "http://management.api/api/HealthCheck/api-status"
      }
    ]
  },
  "Urls" : {
    "IdentityApi" : "http://identity.api"
  }
}
