{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "ConnectionString": "Server=postgres;Port=5432;Database=Joker.Identity;User Id=postgres;Password=postgres;",
  "PersistedGrantConnectionString" : "Server=postgres;Port=5432;Database=Joker.Persisted.Grant;User Id=postgres;Password=postgres;",
  "Serilog": {
    "ApplicationName": "Identity API",
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Enrich": [
      "FromLogContext",
      "WithMachineName"
    ],
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "Seq",
        "Args": {
          "ServerUrl": "http://seq"
        }
      }
    ]
  },
  "ServiceDiscovery": {
    "ServiceId": "Identity.Api-Development",
    "ServiceName": "Identity.Api",
    "ConsulUrl": "http://consul:8500",
    "Address": "identity.api",
    "Port": 80,
    "Endpoints": [

    ]
  },
  "JokerWebAppUrl" : "http://localhost:5030"
}
