{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "ConnectionString": "Server=postgres;Port=5432;Database=Joker.Location;User Id=postgres;Password=postgres;",
  "Serilog": {
    "ApplicationName": "Location API",
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Enrich": [ "FromLogContext", "WithMachineName" ],
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "Seq",
        "Args": { "ServerUrl": "http://seq" }
      }
    ]
  },
  "ServiceDiscovery": {
    "ServiceId": "Location.Api-Docker",
    "ServiceName": "Location.Api",
    "ConsulUrl": "http://consul:8500",
    "Address": "http://location.api",
    "Port": 80,
    "Endpoints": [
      {
        "Url": "http://location.api/api/HealthCheck/api-status"
      }
    ]
  }
}
