{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "ConnectionString": "Server=localhost;Port=5432;Database=Joker.Location;User Id=postgres;Password=postgres;",
  "Serilog": {
    "ApplicationName": "Location API",
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Enrich": [
      "FromLogContext",
      "WithMachineName"
    ],
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "Seq",
        "Args": {
          "ServerUrl": "http://localhost:5341"
        }
      }
    ]
  },
  "ServiceDiscovery": {
    "ServiceId": "Location.Api-Development",
    "ServiceName": "Location.Api",
    "ConsulUrl": "http://localhost:8500",
    "Address": "http://localhost",
    "Port": 5003,
    "Endpoints": [
      {
        "Url": "http://localhost:5003/api/HealthCheck/api-status"
      }
    ]
  }
}
