{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Mongo": {
    "ConnectionString": "mongodb://nosqldata",
    "DefaultDatabaseName": "campaign-db"
  },
  "RabbitMQSettings": {
    "Url": "rabbitmq:5672",
    "Username": "guest",
    "Password": "guest",
    "Port": 5672,
    "Host": "rabbitmq"
  },
  "Serilog": {
    "ApplicationName": "Campaign API",
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Enrich": [
      "FromLogContext",
      "WithMachineName"
    ],
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "Seq",
        "Args": {
          "ServerUrl": "http://seq"
        }
      }
    ]
  },
  "ServiceDiscovery": {
    "ServiceId": "Campaign.Api-Docker",
    "ServiceName": "Campaign.Api",
    "ConsulUrl": "http://consul:8500",
    "Address": "campaign-api",
    "Port": 80,
    "Endpoints": [
      {
        "Url": "http://campaign-api/api/HealthCheck/api-status"
      }
    ]
  },
  "Urls" : {
    "IdentityApi" : "http://identity-api"
  }
}
