{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "ServiceUrls": {
    "merchant": "http://merchant-api:81",
    "campaign": "http://campaign-api:81",
    "management": "http://management-api:81",
    "location": "http://location-api:81",
    "favorite": "http://favorite-api:81"
  },
  "Serilog": {
    "ApplicationName": "Aggregator API",
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Enrich": [
      "FromLogContext",
      "WithMachineName"
    ],
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "Seq",
        "Args": {
          "ServerUrl": "http://seq"
        }
      }
    ]
  },
  "ServiceDiscovery": {
    "ServiceId": "Aggregator.Api-Development",
    "ServiceName": "Aggregator.Api",
    "ConsulUrl": "http://consul:8500",
    "Address": "aggregator-api",
    "Port": 80,
    "Endpoints": [
      {
        "Url": "http://aggregator-api/api/HealthCheck/api-status"
      }
    ]
  },
  "Urls" : {
    "IdentityApi" : "http://identity-api"
  }
}
