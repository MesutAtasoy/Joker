{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "ServiceUrls": {
    "merchant": "http://merchant.api",
    "campaign": "http://campaign.api",
    "management": "http://management.api",
    "location": "http://location.api"
  },
  "Serilog": {
    "ApplicationName": "Aggregator API",
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Enrich": [
      "FromLogContext",
      "WithMachineName"
    ],
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "Seq",
        "Args": {
          "ServerUrl": "http://seq"
        }
      }
    ]
  },
  "ServiceDiscovery": {
    "ServiceId": "Aggregator.Api-Development",
    "ServiceName": "Aggregator.Api",
    "ConsulUrl": "http://consul:8500",
    "Address": "http://aggregator.api",
    "Port": 80,
    "Endpoints": [
      {
        "Url": "http://aggregator.api/api/HealthCheck/api-status"
      }
    ]
  }
}
